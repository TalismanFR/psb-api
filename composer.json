{
    "name": "talismanfr/psb-api",
    "description": "Library for working with API PSB Bank.",
    "type": "library",
    "keywords": [
        "psbbank",
        "api"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Anatoliy Egorov",
            "email": "anat-gor@ya.ru",
            "role": "onedeveloper"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "talismanfr\\psbbank\\": "src"
        }
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR2"
    }
}
